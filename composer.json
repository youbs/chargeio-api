{
  "name": "youbs/chargeio-api",
  "type": "library",
  "description": "A PHP client for ChargeIO API",
  "keywords": [
    "chargeio",
    "affinipay",
    "client"
  ],
  "homepage": "https://github.com/youbs/chargeio-api",
  "license": "MIT",
  "authors": [
    {
      "name": "Hubert Moutot",
      "homepage": "https://github.com/youbs"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/paylike/php-api/contributors"
    }
  ],
  "require": {
    "php": "^5.6|7.*",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Youbs\\ChargeIO\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "prefer-stable": true
}
